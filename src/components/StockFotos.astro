<section id="stockFotos">
  <div id="img_container">
    <img src="../../public/assets/stockimg/stock1.webp" alt="et stockfoto ved navn sammenhold, af fire hænder der holder hinandens håndled" class="vertical" id="sammenhold" />
    <img src="../../public/assets/stockimg/stock2.webp" alt="et stockfoto ved navn arbejde, af to kvinder ved en bærbarcomputer" class="horizontal" id="arbejde1" />
