    border: 1px cyan dotted;
  }
  #img_container {
    width: 66%;
    height: 66%;
  }
  .horizontal {
    width: 30vw;
  }
  .vertical {
    height: 30vw;
  }

  #sammenhold {
    transform: rotate(-5.803deg);
  }
  #arbejde1 {
    transform: rotate(-3.167deg);
  }

  #arbejde2 {
    transform: rotate(7.004deg);
  }
